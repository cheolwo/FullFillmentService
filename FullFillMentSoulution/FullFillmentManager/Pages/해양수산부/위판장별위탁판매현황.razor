@page "/위판장별위탁판매현황Page"
@using 수협FrontCommon.ViewModel.Sample;

@inject 위판장별위탁판매현황ViewModel _위판장별위탁판매현황ViewModel
<MudText Align="Align.Center" Typo="Typo.h2">위판장별위탁판매현황</MudText>
@foreach (var item in _위판장별위탁판매현황ViewModel.Items)
{
    <MudCard Outlined="true">
        <MudCardMedia Image="/images/창고.png" Height="400" />
        <MudCardHeader>
            <CardHeaderContent>
                <MudText Typo="Typo.h6">@item.MxtrNm</MudText>
            </CardHeaderContent>
            <CardHeaderActions>
                <MudIconButton Icon="@Icons.Material.Filled.Settings" Color="Color.Default" />
            </CardHeaderActions>
        </MudCardHeader>
        <MudCardContent>
            <MudText Typo="Typo.body2">위판장별위탁판매현황코드 : @item.MxtrCode</MudText>
            <MudText Typo="Typo.body2">FshrCode : @item.FshrCode</MudText>
            <MudText Typo="Typo.body2">MprcStdCodeNm : @item.MprcStdCodeNm</MudText>
        </MudCardContent>
        <MudCardActions>
            <MudButton Variant="Variant.Text" Color="Color.Primary">Read More</MudButton>
        </MudCardActions>
    </MudCard>
}
@code {
    protected override async Task OnInitializedAsync()
    {
        await _위판장별위탁판매현황ViewModel.Load위판장별위탁판매현황정보();
    }
}
