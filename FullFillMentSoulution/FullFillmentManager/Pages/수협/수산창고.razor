@*@page "/수산창고Management"
@using 수협시스템관리자Common.ViewModel;
@inject NavigationManager _navigationManager
@inject 수산창고ViewModel _수산창고ViewModel
<MudText Align="Align.Center" Typo="Typo.h2">산지조합창고</MudText>
@foreach (var item in _수산창고ViewModel.수산창고List)
{
    <MudCard Outlined="true">
        <MudCardMedia Image="/images/창고.png" Height="300" />
        <MudCardHeader>
            <CardHeaderContent>
                <MudText Typo="Typo.h6">@item.Name</MudText>
            </CardHeaderContent>
            <CardHeaderActions>
                <MudIconButton Icon="@Icons.Material.Filled.Settings" Color="Color.Default" />
            </CardHeaderActions>
        </MudCardHeader>
        <MudCardContent>
            <MudText Typo="Typo.body2">산지조합창고코드 : @item.Code</MudText>
        </MudCardContent>
        <MudCardActions>
            <MudButton Variant="Variant.Text" Color="Color.Primary">관리품목</MudButton>
        </MudCardActions>
    </MudCard>
}
@code {
    protected override async Task OnInitializedAsync()
    {
        await _수산창고ViewModel.Load수산창고Async();
    }
}*@